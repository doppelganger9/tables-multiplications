{
  "name": "tables-multiplications",
  "version": "0.0.1",
  "description": "An angular App, to learm multiplication tables",
  "private": true,
  "scripts": {
    "ng": "ng",
    "prestart": "node ./build/git-info.mjs",
    "start": "ng serve",
    "prebuild": "node ./build/git-info.mjs",
    "build": "ng build",
    "test": "TZ=UTC npx jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --runInBand",
    "lint": "npx eslint 'src/**/*.{js,jsx,ts,tsx,html}' --quiet --fix",
    "lint:css": "npx stylelint '**/*.css'",
    "format": "npx prettier 'src/**/*.{js,jsx,ts,tsx,html}' --write",
    "e2e": "ng e2e",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "prewc:build": "mkdir -p static/ && node ./build/git-info.mjs",
    "wc:build": "ng build tables-multiplications",
    "wc:copy-static": "cp ./dist/tables-multiplications/browser/* ./static/",
    "prewc:serve": "npm run wc:build && npm run wc:copy-static",
    "wc:serve": "npx http-server static"
  },
  "dependencies": {
    "@angular/animations": "17.2.2",
    "@angular/common": "17.2.2",
    "@angular/compiler": "17.2.2",
    "@angular/core": "17.2.2",
    "@angular/elements": "17.2.2",
    "@angular/forms": "17.2.2",
    "@angular/platform-browser": "17.2.2",
    "@angular/platform-browser-dynamic": "17.2.2",
    "@angular/router": "17.2.2",
    "rxjs": "7.8.1",
    "tslib": "2.6.2",
    "zone.js": "0.14.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "17.2.0",
    "@angular-eslint/builder": "17.2.1",
    "@angular-eslint/eslint-plugin": "17.2.1",
    "@angular-eslint/eslint-plugin-template": "17.2.1",
    "@angular-eslint/schematics": "17.2.1",
    "@angular-eslint/template-parser": "17.2.1",
    "@angular/cli": "17.2.0",
    "@angular/compiler-cli": "17.2.2",
    "@angular/localize": "17.2.2",
    "@cypress/schematic": "2.5.1",
    "@types/jest": "29.5.12",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "cypress": "latest",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "jest": "29.7.0",
    "jest-preset-angular": "13.1.6",
    "local-cypress": "1.2.6",
    "prettier": "3.2.5",
    "stylelint": "16.2.1",
    "stylelint-config-prettier": "9.0.5",
    "stylelint-config-standard": "35.0.0",
    "ts-jest": "29.1.2",
    "typescript": "5.3.3"
  }
}