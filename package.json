{
  "name": "tables-multiplications",
  "version": "0.0.1",
  "description": "An angular App, to learm multiplication tables",
  "private": true,
  "scripts": {
    "ng": "ng",
    "prestart": "node ./build/git-info.mjs",
    "start": "ng serve",
    "prebuild": "node ./build/git-info.mjs",
    "build": "ng build",
    "test": "TZ=UTC npx jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --runInBand",
    "lint": "npx eslint 'src/**/*.{js,jsx,ts,tsx,html}' --quiet --fix",
    "lint:css": "npx stylelint '**/*.css'",
    "format": "npx prettier 'src/**/*.{js,jsx,ts,tsx,html}' --write",
    "test:e2e": "start-server-and-test start http://localhost:4200 cypress:run",
    "e2e": "ng e2e",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "prewc:build": "mkdir -p static/ && node ./build/git-info.mjs",
    "wc:build": "ng build tables-multiplications",
    "wc:copy-static": "cp ./dist/tables-multiplications/browser/* ./static/",
    "prewc:serve": "npm run wc:build && npm run wc:copy-static",
    "wc:serve": "npx http-server static"
  },
  "dependencies": {
    "@angular/animations": "18.2.14",
    "@angular/common": "18.2.14",
    "@angular/compiler": "18.2.14",
    "@angular/core": "18.2.14",
    "@angular/elements": "18.2.14",
    "@angular/forms": "18.2.14",
    "@angular/platform-browser": "18.2.14",
    "@angular/platform-browser-dynamic": "18.2.14",
    "@angular/router": "18.2.14",
    "rxjs": "7.8.2",
    "tslib": "2.8.1",
    "zone.js": "0.16.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "18.2.21",
    "@angular-eslint/builder": "19.8.1",
    "@angular-eslint/eslint-plugin": "19.8.1",
    "@angular-eslint/eslint-plugin-template": "19.8.1",
    "@angular-eslint/schematics": "19.8.1",
    "@angular-eslint/template-parser": "19.8.1",
    "@angular/cli": "18.2.21",
    "@angular/compiler-cli": "18.2.14",
    "@angular/localize": "18.2.14",
    "@cypress/schematic": "4.3.0",
    "@eslint/eslintrc": "3.3.3",
    "@eslint/js": "9.39.2",
    "@types/jest": "29.5.14",
    "@typescript-eslint/eslint-plugin": "8.53.0",
    "@typescript-eslint/parser": "8.53.0",
    "@typescript-eslint/utils": "8.53.0",
    "cypress": "latest",
    "eslint": "9.39.2",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "5.5.5",
    "globals": "16.0.0",
    "jest": "29.7.0",
    "jest-preset-angular": "14.6.2",
    "local-cypress": "1.2.8",
    "prettier": "3.8.0",
    "start-server-and-test": "2.1.3",
    "stylelint": "16.26.1",
    "stylelint-config-standard": "39.0.1",
    "ts-jest": "29.4.6",
    "typescript": "5.4.5"
  }
}
