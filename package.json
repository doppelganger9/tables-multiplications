{
  "name": "tables-multiplications",
  "version": "0.0.0",
  "description": "An angular App, to learm multiplication tables",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "npx jest",
    "lint": "npx eslint 'src/**/*.{js,jsx,ts,tsx,html,css,scss}' --quiet --fix",
    "format": "npx prettier 'src/**/*.{js,jsx,ts,tsx,html,css,scss}' --write",
    "e2e": "ng e2e",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "wc:build": "npm run wc:build-task && npm run wc:package-task",
    "wc:build-task": "ng build tables-multiplications --output-hashing=none",
    "wc:package-task": "cat ./dist/tables-multiplications/{polyfills,runtime,main}.js > ./dist/table-multiplication-app.js",
    "wc:copy-static": "cp ./dist/table-multiplication-app.js ./static/",
    "wc:serve": "npm run wc:build && && npm run wc:copy-static && npx http-server static"
  },
  "dependencies": {
    "@angular/animations": "^15.1.2",
    "@angular/common": "^15.1.2",
    "@angular/compiler": "^15.1.2",
    "@angular/core": "^15.1.2",
    "@angular/elements": "^15.1.2",
    "@angular/forms": "^15.1.2",
    "@angular/platform-browser": "^15.1.2",
    "@angular/platform-browser-dynamic": "^15.1.2",
    "@angular/router": "^15.1.2",
    "@webcomponents/webcomponentsjs": "^2.7.0",
    "rxjs": "^7.8.0",
    "tslib": "^2.4.1",
    "zone.js": "^0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.1.3",
    "@angular-eslint/builder": "15.2.0",
    "@angular-eslint/eslint-plugin": "15.2.0",
    "@angular-eslint/eslint-plugin-template": "15.2.0",
    "@angular-eslint/schematics": "15.2.0",
    "@angular-eslint/template-parser": "15.2.0",
    "@angular/cli": "~15.1.3",
    "@angular/compiler-cli": "^15.1.2",
    "@cypress/schematic": "2.5.0",
    "@types/jest": "^28.0.0",
    "@typescript-eslint/eslint-plugin": "5.48.1",
    "@typescript-eslint/parser": "5.48.1",
    "cypress": "latest",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-prettier": "4.2.1",
    "jest": "^28.0.0",
    "jest-preset-angular": "^12.2.6",
    "local-cypress": "1.2.6",
    "prettier": "2.8.3",
    "ts-jest": "28.0.8",
    "typescript": "~4.8.0"
  }
}
