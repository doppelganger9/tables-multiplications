{
  "name": "tables-multiplications",
  "version": "0.0.1",
  "description": "An angular App, to learm multiplication tables",
  "private": true,
  "scripts": {
    "ng": "ng",
    "prestart": "node ./build/git-info.mjs",
    "start": "ng serve",
    "prebuild": "node ./build/git-info.mjs",
    "build": "ng build",
    "test": "npx jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --runInBand",
    "lint": "npx eslint 'src/**/*.{js,jsx,ts,tsx,html}' --quiet --fix",
    "lint:css": "npx stylelint '**/*.css'",
    "format": "npx prettier 'src/**/*.{js,jsx,ts,tsx,html}' --write",
    "e2e": "ng e2e",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "wc:build": "npm run wc:build-task && npm run wc:package-task",
    "wc:build-task": "ng build tables-multiplications --output-hashing=none",
    "wc:package-task": "node ./build/concat.mjs",
    "wc:copy-static": "cp ./dist/table-multiplication-app.js ./static/",
    "wc:serve": "npm run wc:build && npm run wc:copy-static && npx http-server static"
  },
  "dependencies": {
    "@angular/animations": "16.2.10",
    "@angular/common": "16.2.10",
    "@angular/compiler": "16.2.10",
    "@angular/core": "16.2.10",
    "@angular/elements": "16.2.10",
    "@angular/forms": "16.2.10",
    "@angular/platform-browser": "16.2.10",
    "@angular/platform-browser-dynamic": "16.2.10",
    "@angular/router": "16.2.10",
    "@webcomponents/webcomponentsjs": "2.8.0",
    "rxjs": "7.8.1",
    "tslib": "2.6.2",
    "zone.js": "0.14.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "16.2.6",
    "@angular-eslint/builder": "16.2.0",
    "@angular-eslint/eslint-plugin": "16.2.0",
    "@angular-eslint/eslint-plugin-template": "16.2.0",
    "@angular-eslint/schematics": "16.2.0",
    "@angular-eslint/template-parser": "16.2.0",
    "@angular/cli": "16.2.6",
    "@angular/compiler-cli": "16.2.10",
    "@cypress/schematic": "2.5.1",
    "@types/jest": "29.5.6",
    "@typescript-eslint/eslint-plugin": "6.8.0",
    "@typescript-eslint/parser": "6.8.0",
    "cypress": "latest",
    "eslint": "8.51.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-prettier": "5.0.1",
    "jest": "29.7.0",
    "jest-preset-angular": "13.1.2",
    "local-cypress": "1.2.6",
    "prettier": "3.0.3",
    "stylelint": "15.11.0",
    "stylelint-config-prettier": "9.0.5",
    "stylelint-config-standard": "34.0.0",
    "ts-jest": "29.1.1",
    "typescript": "~5.1.0"
  }
}
