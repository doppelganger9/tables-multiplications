{
  "name": "tables-multiplications",
  "version": "0.0.1",
  "description": "An angular App, to learm multiplication tables",
  "private": true,
  "scripts": {
    "ng": "ng",
    "prestart": "node ./build/git-info.mjs",
    "start": "ng serve",
    "prebuild": "node ./build/git-info.mjs",
    "build": "ng build",
    "test": "TZ=UTC npx jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --runInBand",
    "lint": "npx eslint 'src/**/*.{js,jsx,ts,tsx,html}' --quiet --fix",
    "lint:css": "npx stylelint '**/*.css'",
    "format": "npx prettier 'src/**/*.{js,jsx,ts,tsx,html}' --write",
    "test:e2e": "start-server-and-test start http://localhost:4200 cypress:run",
    "e2e": "ng e2e",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "prewc:build": "mkdir -p static/ && node ./build/git-info.mjs",
    "wc:build": "ng build tables-multiplications",
    "wc:copy-static": "cp ./dist/tables-multiplications/browser/* ./static/",
    "prewc:serve": "npm run wc:build && npm run wc:copy-static",
    "wc:serve": "npx http-server static"
  },
  "dependencies": {
    "@angular/animations": "18.2.7",
    "@angular/common": "18.2.7",
    "@angular/compiler": "18.2.7",
    "@angular/core": "18.2.7",
    "@angular/elements": "18.2.7",
    "@angular/forms": "18.2.7",
    "@angular/platform-browser": "18.2.7",
    "@angular/platform-browser-dynamic": "18.2.7",
    "@angular/router": "18.2.7",
    "rxjs": "7.8.1",
    "tslib": "2.7.0",
    "zone.js": "0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "18.2.7",
    "@angular-eslint/builder": "17.5.3",
    "@angular-eslint/eslint-plugin": "17.5.3",
    "@angular-eslint/eslint-plugin-template": "17.5.3",
    "@angular-eslint/schematics": "17.5.3",
    "@angular-eslint/template-parser": "17.5.3",
    "@angular/cli": "18.2.7",
    "@angular/compiler-cli": "18.2.7",
    "@angular/localize": "18.2.7",
    "@cypress/schematic": "2.5.1",
    "@types/jest": "29.5.12",
    "@typescript-eslint/eslint-plugin": "8.8.0",
    "@typescript-eslint/parser": "8.8.0",
    "cypress": "latest",
    "eslint": "9.11.1",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "jest": "29.7.0",
    "jest-preset-angular": "14.2.4",
    "local-cypress": "1.2.6",
    "prettier": "3.3.3",
    "start-server-and-test": "2.0.8",
    "stylelint": "16.9.0",
    "stylelint-config-standard": "36.0.1",
    "ts-jest": "29.2.5",
    "typescript": "5.4.5"
  }
}
